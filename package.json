{
  "name": "tm-libs",
  "version": "1.2.3",
  "description": "tm-libs",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "node build.mjs",
    "build:cp": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias && npm run copy",
    "test": "ts-node test/test.ts",
    "remove": "@powershell rm -r -fo dist",
    "copy": "@powershell copy package.json ./dist && @powershell copy README.md ./dist",
    "publish": "npm publish",
    "publish:lib": "npm run build && cd dist && npm publish"
  },
  "prepare": "npm run build:tsc",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuanmjnh/tm-libs.git"
  },
  "keywords": [
    "tm-libs"
  ],
  "author": "tuanmjnh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tuanmjnh/tm-libs/issues"
  },
  "homepage": "https://github.com/tuanmjnh/tm-libs#readme",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "crypto": "^1.0.1",
    "keytar": "^7.9.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/bcrypt": "^6.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.6.2",
    "esbuild": "^0.25.10",
    "glob": "^11.0.3",
    "npm-dts": "^1.3.13",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "overrides": {
    "tmp": "^0.2.4"
  },
  "browser": {
    "node:fs": false,
    "node:path": false
  },
  "module": "dist/index.js"
}