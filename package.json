{
  "name": "tm-libs",
  "version": "1.3.8",
  "description": "tm-libs",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "node build.mjs",
    "build:cp": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias && npm run copy",
    "test": "ts-node --esm test/test.ts",
    "test:npx": "npx ts-node test/test.ts",
    "remove": "@powershell rm -r -fo dist",
    "copy": "@powershell copy package.json ./dist && @powershell copy README.md ./dist",
    "publish:lib": "npm run build && cd dist && npm publish",
    "git:cache": "git rm -r --cached . && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuanmjnh/tm-libs.git"
  },
  "keywords": [
    "tm-libs"
  ],
  "author": "tuanmjnh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tuanmjnh/tm-libs/issues"
  },
  "homepage": "https://github.com/tuanmjnh/tm-libs#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "crypto-js": "^4.2.0",
    "keytar": "^7.9.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@types/bcrypt": "^6.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.6.2",
    "esbuild": "^0.25.10",
    "glob": "^11.0.3",
    "npm-dts": "^1.3.13",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "overrides": {
    "tmp": "^0.2.4"
  }
}