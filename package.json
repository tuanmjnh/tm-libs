{
  "name": "tm-libs",
  "version": "1.1.9",
  "description": "tm-libs",
  "main": "index.ts",
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "index.esm.js",
    "fs-lib.js",
    "fs-lib.d.ts",
    "README.md"
  ],
  "type": "commonjs",
  "exports": {
    "types": "./index.d.ts",
    "default": "./index.js"
  },
  "prepare": "npm run build:tsc",
  "scripts": {
    "build": "node build.js && npm run copy",
    "build:cp": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias && npm run copy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "remove": "@powershell rm -r -fo dist",
    "copy": "@powershell copy package.json ./dist && @powershell copy README.md ./dist",
    "publish": "cd dist && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuanmjnh/tm-libs.git"
  },
  "keywords": [
    "tm-libs"
  ],
  "author": "tuanmjnh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tuanmjnh/tm-libs/issues"
  },
  "homepage": "https://github.com/tuanmjnh/tm-libs#readme",
  "dependencies": {
    "crypto-js": "^4.2.0",
    "lodash.keyby": "^4.6.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash.keyby": "^4.6.9",
    "@types/node": "^22.10.6",
    "esbuild": "^0.25.5",
    "npm-dts": "^1.3.13",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "browser": {
    "node:fs": false,
    "node:path": false
  }
}